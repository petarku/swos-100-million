{
  "name": "swos-100-million",
  "version": "1.0.0",
  "description": "A simple hex editor using Electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . HexEditor --platform=win32 --arch=x64 --overwrite",
    "package-mac": "electron-packager . HexEditor --platform=darwin --arch=x64 --overwrite"
  },
  "keywords": [
    "electron",
    "hex",
    "editor"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "electron": "^26.2.2",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "@electron/remote": "^2.0.11"
  }
}
